{
  "name": "console-feed",
  "version": "2.8.10",
  "main": "dist/index.js",
  "description": "A React component that displays console logs from the current page, an iframe or transported across a server",
  "module": "dist/console-feed2.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint",
    "clean": "nwb clean-module && nwb clean-demo",
    "release": "yarn test && yarn clean && yarn build && yarn publish && yarn clean",
    "precommit": "pretty-quick --staged",
    "test:coverage": "tsdx test --env=jsdom --coverage",
    "test:watch": "tsdx test --env=jsdom --watch"
  },
  "dependencies": {
    "@emotion/core": "^10.0.17",
    "emotion": "^10.0.17",
    "emotion-theming": "^10.0.19",
    "linkifyjs": "^2.1.6",
    "react-emotion": "^10.0.0",
    "react-inspector": "^4.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.140",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^3.0.5",
    "immutability-helper": "^3.0.1",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.2",
    "tsdx": "^0.9.2",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./scripts/test.ts"
    ]
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "keywords": [
    "devtools",
    "inspector",
    "object",
    "object-inspector",
    "react",
    "react-component",
    "reactjs",
    "table",
    "table-inspector",
    "table-view",
    "tableview",
    "tree",
    "tree-view",
    "treeview",
    "ui",
    "view"
  ],
  "homepage": "https://github.com/samdenty99/console-feed",
  "repository": {
    "type": "git",
    "url": "https://github.com/samdenty99/console-feed.git"
  },
  "bugs": {
    "url": "https://github.com/samdenty99/console-feed/issues"
  },
  "author": "Sam Denty <samddenty@gmail.com> (http://github.com/samdenty99)",
  "license": "MIT"
}
